{
    "env": {
        "browser": true,
        "es2021": true
    },
    
        "overrides": [
          {
            "files": [
              "**/*.test.js",
              "**/*.test.jsx"
            ],
            "env": {
              "jest": true
            }
          }
        ]
      ,
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}

